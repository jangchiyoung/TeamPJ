<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- sql mapper   -->  
<mapper namespace="productDto"> <!-- namespace : 일종의 식별역할의 변수 -->

	<insert id="insert" parameterType="productDto" useGeneratedKeys="true"
				keyColumn="idx" keyProperty="idx">
		insert into member(member_id,member_password,member_name,member_tel,member_email,member_address)
		values (#{member_id},#{member_password},#{member_name},#{member_tel},#{member_email},#{member_address})
	</insert>
	
</mapper>
<!-- ${ } : 표현식. 중괄호안에 수식에 대한 연산결과값을 구해서 출력
     #{ } : 무조건 문자열(' '기호사용)로 중괄호 안의 값을 처리합니다. -->